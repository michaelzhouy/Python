      "10\n",
      "2\n",
      "5\n",
      "6\n",
      "11\n"
     ]
    }
   ],
   "source": [
    "from itertools import chain\n",
    "\n",
    "# chain()会先输出第一个列表，再接着输出第二个列表\n",
    "for c in chain(a, b):\n",
    "    print(c)"
   ]
  }
